#TabbedPanelTabs {
    float:left;
    width:100%;
    background:#EFF4FA;
    font-size:93%;
    line-height:normal;
    border-bottom:1px solid #DD740B;
}
#TabbedPanelTabs ul {
    margin:0;
    padding:10px 10px 0 50px;
    list-style:none;
}
#TabbedPanelTabs li {
    display:inline;
    margin:0;
    padding:0;
}
#TabbedPanelTabs a {
    float:left;
    background: url(tableft.gif) no-repeat left top;
    margin:0;
    padding:0 0 0 5px;
    text-decoration:none;
}
#TabbedPanelTabs a span {
    float:left;
    display:block;
    background:url('tabright.gif') no-repeat right top;
    padding:5px 15px 4px 6px;
    color:#FFF;
}

#TabbedPanelTabs li.selected a {
    float:left;
    background:url("tableft.gif") no-repeat left top;
    margin:0;
    padding:0 0 0 5px;
    text-decoration:none;
    background-position:0% -42px;
}
#TabbedPanelTabs li.selected a span {
    float:left;
    display:block;
    background:url("tabright.gif") no-repeat right top;
    padding:5px 15px 4px 6px;
    color:#FFF;
    background-position:100% -42px;
}
/* Commented Backslash Hack hides rule from IE5-Mac \*/
#TabbedPanelTabs a span {float:none;}
/* End IE5-Mac hack */
#TabbedPanelTabs a:hover span {
    color:#FFF;
}
#TabbedPanelTabs a:hover {
    background-position:0% -42px;
}
#TabbedPanelTabs a:hover span {
    background-position:100% -42px;
}

.tabPanel {
    border: 1px solid #DD740B;
    padding:5px 5px 5px 5px;
}

